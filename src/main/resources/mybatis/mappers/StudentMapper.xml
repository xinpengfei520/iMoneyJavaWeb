<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xpf.imoney.mapper.StudentMapper">
    <select id="queryAllStudents" resultType="com.xpf.imoney.bean.Student">
        select * from students
    </select>
    <select id="findStudentById" parameterType="java.lang.Integer" resultType="com.xpf.imoney.bean.Student">
        select * from students where id = #{id}
    </select>
</mapper>